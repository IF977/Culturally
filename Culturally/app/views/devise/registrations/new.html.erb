        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= devise_error_messages! %>

          <div class="p-t-31 p-b-9">
            <%= f.label :email, :class => 'txt1' %>
          </div>

          <div class="wrap-input100 validate-input" data-validate = "Username is required">
 
            <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => 'input100'%>

          </div>

          <div class="p-t-13 p-b-9">
     
            <%= f.label :password, :class => 'txt1' %>

            <!--a href="#" class="txt2 bo1 m-l-5">
              Forgot?
            </a-->
          </div>


          <div class="wrap-input100" >
         
            <%= f.password_field :password, autocomplete: "off", :class => 'input100' %>
         
          </div>

          <% if @minimum_password_length %>
            <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />

          <div class="p-t-13 p-b-9 field">
     
             <%= f.label :password_confirmation , :class => 'txt1'%>

          </div>
    
          <div class="wrap-input100 field" >
         
         
            <%= f.password_field :password_confirmation, autocomplete: "off", :class => 'input100' %>
         
          </div>

          <div class="container-login100-form-btn m-t-17 actions">
    
            <%= f.submit "Sign up", :class => 'login100-form-btn' %>

          </div>

        <% end %>

